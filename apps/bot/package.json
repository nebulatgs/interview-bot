{
	"name": "bot",
	"version": "1.0.0",
	"private": true,
	"license": "MIT",
	"type": "module",
	"main": "build/main.js",
	"scripts": {
		"build": "tsc",
		"build:changelog": "npx @discordx/changelog --root=src",
		"dev": "ts-node-esm src/main.ts",
		"start": "node build/main.js",
		"watch": "nodemon --exec ts-node-esm src/main.ts",
		"migrate": "prisma migrate dev",
		"generate": "prisma generate"
	},
	"dependencies": {
		"@discordx/importer": "^1.1.10",
		"@discordx/pagination": "^3.3.1",
		"@prisma/client": "^4.7.1",
		"discord.js": "^14.6.0",
		"discordx": "^11.4.0",
		"ws": "^8.11.0"
	},
	"devDependencies": {
		"@types/node": "^18.11.7",
		"@types/ws": "^8.5.3",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"prisma": "^4.7.1",
		"ts-node": "^10.9.1",
		"typescript": "4.8.4"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.0.0"
	}
}
